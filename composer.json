{
  "name": "miralba/mh13",
  "description": "Legacy app migration",
  "type": "project",
  "require": {
    "symfony/http-foundation": "^3.2",
    "twig/twig": "^1.31",
    "silex/silex": "2.1.x-dev",
    "symfony/console": "^3.2",
    "doctrine/dbal": "^2.5",
    "symfony/yaml": "3.2.x-dev",
    "ramsey/uuid": "4.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "phpspec/phpspec": "^3.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "franiglesias",
      "email": "franiglesias@mac.com"
    }
  ],
  "autoload": {
    "classmap": [
      "plugins/",
      "app_model.php",
      "app_controller.php",
      "app_helper.php",
      "app_error.php",
      "vendors/cakephp/"
    ],
    "exclude-from-classmap": [
      "/vendors/cakephp/cake/libs/model/app_model.php",
      "/vendors/cakephp/cake/libs/controller/app_controller.php",
      "/vendors/cakephp/cake/libs/view/helpers/app_helper.php",
      "vendors/cakephp/cake/console/",
      "/tests/",
      "/spec/",
      "/src/"
    ],
    "psr-4": {
      "Mh13\\": [
        "src/Mh13/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": [
        "src/tests/"
      ]
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
